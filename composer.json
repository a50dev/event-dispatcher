{
    "name": "a50/event-dispatcher",
    "description": "Event Dispatcher",
    "keywords": [
        "a50",
        "event dispatcher",
        "psr-14"
    ],
    "homepage": "https://github.com/a50/event-dispatcher",
    "license": "MIT",
    "authors": [
        {
            "name": "Siarhei Bautrukevich",
            "email": "hello@bautrukevich.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/event-dispatcher": "^1.0",
        "webmozart/assert": "^1.11",
        "a50/container": "dev-main",
        "innmind/immutable": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "A50\\EventDispatcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "A50\\EventDispatcher\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
